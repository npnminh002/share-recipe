<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#30DFCFFF" />
            <corners android:radius="@dimen/_12sdp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#50BFCFFF" />
            <corners android:radius="@dimen/_12sdp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="3dp"

       >
        <shape android:shape="rectangle">
            <solid android:color="#70AFCFFF" />
            <corners android:radius="@dimen/_12sdp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp">
        <shape android:shape="rectangle">
            <gradient
                android:endColor="@color/appcolor"
                android:startColor="@color/blue" />
            <corners android:radius="@dimen/_12sdp" />
        </shape>
    </item>
</layer-list>